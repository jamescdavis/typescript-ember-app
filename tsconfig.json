{
  "compilerOptions": {
    "target": "es2017",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noImplicitThis": true,
    "noEmitOnError": false,
    "noEmit": true,
    "sourceMap": true,
    "baseUrl": ".",
    "module": "es6",
    "paths": {
      "typescript-ember-app/tests/*": [
        "tests/*"
      ],
      "typescript-ember-app/*": [
        "app/*",
        "node_modules/typescript-ember-addon/app/*"
      ],
      "*": [
        "types/*"
      ],
      "typescript-ember-addon": [
        "node_modules/typescript-ember-addon/addon"
      ],
      "typescript-ember-addon/*": [
        "node_modules/typescript-ember-addon/addon/*"
      ]
    }
  },
  "include": [
    "app",
    "tests",
    "types",
    "node_modules/typescript-ember-addon/app",
    "node_modules/typescript-ember-addon/addon"
  ]
}
